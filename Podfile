#Uncomment the next line to define a global platform for your project
 platform :ios, '9.0'

target 'WhereAtV2' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for WhereAtV2
  pod 'FacebookCore', '~> 0.2'
  pod 'FacebookLogin'
  pod 'FacebookShare'
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  pod 'GooglePlacePicker'


  target 'WhereAtV2Tests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'WhereAtV2UITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
